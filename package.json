{
  "name": "vagrant-microservice",
  "version": "0.0.0",
  "description": "proof of concept micro-service for a rabbitmq cluster ",
  "main": "index.js",
  "scripts": {
    "start": "bash ./services/etc/start.sh",
    "startforever": "bash ./services/etc/startforever.sh",
    "stop": "forever stopall",
    "setup": "python ./services/etc/setup_rabbitmq.py",
    "test": "bash ./services/etc/test.sh"
  },
